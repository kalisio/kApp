
{
  // TODO is it required ?
//  "include": [
//    "./src/**/*"
//  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "directives/*": [
        "src/directives/*"
      ],
      "mixins/*": [
        "src/mixins/*"
      ],
      "plugins/*": [
        "src/plugins/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ]
    }
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ]
}
